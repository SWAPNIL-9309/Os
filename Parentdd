#include<stdio.h>

#include<stdlib.h>

int main(int argc, char*argv[])

{

int arr[50],i;

printf("Enter array elements: \n");

for(i=1;i<=argc-1;i++)

{

arr[i]=atoi(argv[i]);

printf("%d\n",arr[i]);

}

printf("Reverse array: \n");

for(i=argc-1;i>=1;i--)

{

arr[i]=atoi(argv[i]);

printf("%d\n",arr[i]);

}

return 0;

}
